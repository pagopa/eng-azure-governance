{
  "if": {
    "allOf": [
      { "field": "type", "equals": "Microsoft.Web/sites" },
      { "field": "kind", "contains": "functionapp" },
      { "field": "Microsoft.Web/sites/config/minTlsVersion", "notIn": "[parameters('listOfAllowedTLS')]" }
    ]
  },
  "then": {
    "effect": "[parameters('effect')]"
  }
}