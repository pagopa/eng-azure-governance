{
  "if": {
    "allOf": [
      {
        "field": "location",
        "in": "[parameters('listOfLocations')]"
      },
      {
        "field": "type",
        "equals": "Microsoft.OperationalInsights/workspaces"
      },
      {
        "not": {
          "field": "Microsoft.OperationalInsights/workspaces/sku.name",
          "in": "[parameters('listOfAllowedSku')]"
        }
      }
    ]
  },
  "then": {
    "effect": "[parameters('effect')]"
  }
}
